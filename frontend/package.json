    "@vercel/analytics": "1.6.1",
    "@vercel/speed-insights": "1.3.1",
    "astro": "5.16.6",
    "astro-htmx": "1.0.6",
    "astro-portabletext": "0.12.0",
    "htmx.org": "^2.0.6",
    "react": "^19.2.2",
    "react-dom": "^19.1.0",
